		    <!-- /.panel-body -->
		    </div>
		    <!-- /.panel -->
		    </div>
		    <!-- /.col-lg-4 -->
		    «ENDIF»
		    «ENDFOR»
	    </div>
	</div>
	<!-- /#page-wrapper -->
